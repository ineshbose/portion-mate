# SECURITY WARNING: don't run with the debug turned on in production!
DEBUG=True

# SECURITY WARNING: keep the secret key used in production secret!
SECRET_KEY=secret

# A list of all the people who get code error notifications.
ADMINS=John Doe <john@example.com>, Mary <mary@example.com>

# A list of hosts to be allowed for the project
ALLOWED_HOSTS=127.0.0.1,localhost

# API endpoint URL for frontend
API_BASE=http://127.0.0.1/api

# Generated from oauth2_provider from backend
CLIENT_ID=oauth_app_client_id

# Generated from oauth2_provider from backend
CLIENT_SECRET=oauth_app_client_secret

# PostgreSQL parameters
DB_NAME=portionmate
DB_USER=portionmateuser
DB_PASS=password
DB_HOST=127.0.0.1
DB_PORT=5432
